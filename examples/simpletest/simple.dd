# example .dd file

BBQ.header do

  enum Colors, {:red => 0, :blue => 1, :green => 2}

  struct Complex do
    float real, 0
    float imag, 0
  end

  struct Mobius do
    Complex a
    Complex b
    Complex c
    Complex d
    int32 conj, 0
  end

  struct Tile do
    Mobius mobius
  end

  struct Level do
    uint32 cookie, 0xbaadf00d
    Colors color, :red
    fixed_array Tile, tiles, 4
    var_array int32, numbers
  end

end
